{
  "name": "TrustNews",
  "description": "Displays the reliability of a news source",
  "version": "1.1.5",
  "icons": { "128": "icon.png" },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
      "default_title": "Display the news site's ratings.",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2
}
